{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": {
      "branchToUse": "main"
    },
    "reg-notify-github-plugin": {
      "clientId": "${{ secrets.REPOSITORY_CLIENT_ID }}"
    },
    "reg-publish-gcs-plugin": {
      "bucketName": "${{ secrets.GCS_BUCKET_NAME_VRT }}"
    }
  }
}
