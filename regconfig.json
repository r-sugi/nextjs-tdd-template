{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-github-plugin": {
      "clientId": "$REPOSITORY_CLIENT_ID"
    },
    "reg-publish-gcs-plugin": {
      "bucketName": "$GCS_BUCKET_NAME_VRT"
    }
  }
}
