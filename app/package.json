{
  "name": "nextjs-tdd-template",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "^v18.19.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_API_MOCKING=enabled NODE_OPTIONS='--inspect=0.0.0.0' next dev",
    "build": "next build",
    "start": "next start",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome check --apply src",
    "lint:fix:force": "biome check --fix --unsafe src",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.js",
    "storybook": "RUNTIME_ENV=storybook storybook dev -p 6006",
    "storybook:build": "RUNTIME_ENV=storybook storybook build",
    "fn:deploy": "npm run deploy --prefix ./functions",
    "fn:watch": "npm run build:watch --prefix ./functions",
    "db:export": "npm run db:export --prefix ./db",
    "db:apply": "npm run db:apply --prefix ./db",
    "db:console": "npm run db:console --prefix ./db",
    "db:migration": "npm run db:migration --prefix ./db",
    "db:metadata": "npm run db:metadata --prefix ./db",
    "db:meta:export": "npm run db:meta:export --prefix ./db",
    "db:dump": "npm run db:dump --prefix ./db",
    "postinstall": "sed -E -i '' 's/NODE_OPTIONS.*nodeDebugType.*/NODE_OPTIONS = `${NODE_OPTIONS} --${nodeDebugType}=0.0.0.0:9230`;/' node_modules/next/dist/cli/next-dev.js"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@digital-go-jp/tailwind-theme-plugin": "^0.1.15",
    "@hookform/resolvers": "^3.3.4",
    "@sentry/nextjs": "^8.12.0",
    "firebase": "^10.12.2",
    "next": "^14.2.1",
    "pino": "^9.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@chromatic-com/storybook": "^1.5.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@storybook/addon-a11y": "^8.1.6",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "22.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.23.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.17.4",
    "msw": "^2.3.5",
    "msw-storybook-addon": "^2.0.3",
    "next-router-mock": "^0.9.13",
    "pino-pretty": "^11.2.1",
    "postcss": "^8.4.41",
    "storybook": "^8.1.6",
    "storybook-addon-module-mock": "^1.3.4",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "undici": "^5.28.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
