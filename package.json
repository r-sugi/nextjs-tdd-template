{
  "name": "react-swiper-v10",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next src",
    "dev": "NEXT_PUBLIC_API_MOCKING=enabled next dev",
    "dev:all": "npm-run-all -p dev db:start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.js",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:export": "npm run db:export --prefix ./db"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@hookform/resolvers": "^3.3.4",
    "next": "^14.2.1",
    "pino": "^8.20.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "whatwg-fetch": "^3.6.20",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.2",
    "typescript": "^5.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
