{
  "name": "nextjs-tdd-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20.12.0"
  },
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next src",
    "dev": "NEXT_PUBLIC_API_MOCKING=enabled next dev",
    "dev:all": "npm-run-all -p dev db:start",
    "build": "next build",
    "start": "serve out",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.js",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:export": "npm run db:export --prefix ./db",
    "db:apply": "npm run db:apply --prefix ./db",
    "db:dump": "npm run db:dump --prefix ./db"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@hookform/resolvers": "^3.3.4",
    "next": "^14.2.1",
    "pino": "^8.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "serve": "^14.2.1",
    "whatwg-fetch": "^3.6.20",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@swc/core": "^1.4.17",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.3",
    "next-router-mock": "^0.9.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
