{
  "name": "nextjs-tdd-template",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "^v18.19.0"
  },
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next src",
    "dev": "NEXT_PUBLIC_API_MOCKING=enabled next dev",
    "dev:all": "npm-run-all -p dev db:start",
    "build": "next build",
    "start": "serve out",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome check --apply src",
    "lint:fix:force": "biome check --fix --unsafe src",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:console": "npm run db:console --prefix ./db",
    "db:migration": "npm run db:migration --prefix ./db",
    "db:metadata": "npm run db:metadata --prefix ./db",
    "db:meta:export": "npm run db:meta:export --prefix ./db",
    "db:dump": "npm run db:dump --prefix ./db"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@digital-go-jp/tailwind-theme-plugin": "^0.1.15",
    "@hookform/resolvers": "^3.3.4",
    "next": "^14.2.1",
    "pino": "^8.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.4",
    "whatwg-fetch": "^3.6.20",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@chromatic-com/storybook": "^1.5.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@storybook/addon-a11y": "^8.1.6",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@swc/core": "^1.4.17",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.17.4",
    "lefthook": "^1.6.15",
    "msw": "^1.3.3",
    "next-router-mock": "^0.9.13",
    "storybook": "^8.1.6",
    "typescript": "5.4.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
