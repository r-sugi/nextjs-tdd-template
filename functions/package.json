{
  "name": "functions",
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions,auth,firestore",
